<div class="main row container calculate-pensions">
  <div class="wrapper outs-docs">
    <header>CDN Migration Tool</header>
    <form id="download-csv-form" action="#">
      <div id="message" style="display: none">
        <div id="message-screen-mask" class="ui-widget-overlay ui-front"></div>
        <div
          id="message-text1"
          class="
            ui-dialog
            ui-widget
            ui-widget-content
            ui-corner-all
            ui-front
            ui-draggable
            ui-resizable
            waitmessage
          "
        ></div>
        <div
          id="message-text2"
          class="
            ui-dialog
            ui-widget
            ui-widget-content
            ui-corner-all
            ui-front
            ui-draggable
            ui-resizable
            waitmessage
          "
        ></div>
        <div
          id="message-text3"
          class="
            ui-dialog
            ui-widget
            ui-widget-content
            ui-corner-all
            ui-front
            ui-draggable
            ui-resizable
            waitmessage
          "
          style="display: none"
        ></div>
        <div
          class="
            ui-dialog
            ui-widget
            ui-widget-content
            ui-corner-all
            ui-front
            ui-draggable
            ui-resizable
            waitmessage
          "
          id="cancel-btn"
        >
          <!-- <i id="fa-times" class="fas fa-times"></i> -->
          <a class="btn btnCancel">Cancel</a>
        </div>
      </div>
      <input
        class="file-input"
        type="file"
        enctype="multipart/form-data"
        name="filetoupload"
        accept=".csv"
        id="uploaded-zip"
        hidden
      />
      <div id="browse">
        <i class="fas fa-cloud-upload-alt"></i>
        <p>Browse File to Upload</p>
      </div>

      <section class="progress-area"></section>
      <section class="uploaded-area"></section>
      <div class="btn-download-div">
        <a
          class="btn btn-primary btn-download download"
          style="display: none"
          id="download-link"
        >
          Download CSV
        </a>
        <a
          class="btn btn-primary btn-delete delete"
          style="display: none"
          id="delete-link"
        >
          Delete S3 Orphans
        </a>
      </div>

      <div class="refresh-page" style="display: none">
        <a href="#" id="refresh" style="font-size: 12px">
          Upload another csv file
        </a>
      </div>
    </form>
  </div>
</div>
